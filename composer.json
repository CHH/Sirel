{
    "name": "chh/sirel",
    "description": "Flexible SQL builder which builds an Abstract Syntax Tree of SQL and dumps it.",
    "keywords": [
        "sql", "database"
    ],
    "license": "MIT",
    "homepage": "https://github.com/CHH/sirel",
    "authors": [
        {
            "name": "Christoph Hochstrasser",
            "email": "christoph.hochstrasser@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "doctrine/dbal": "2.*@stable",
        "phpunit/phpunit": "~3.7",
        "phpunit/dbunit": "~1.2",
        "chh/bob": "~1.0@dev"
    },
    "suggest": {
        "doctrine/dbal": "For the Sirel\\Visitor\\DoctrinePlatform visitor."
    },
    "autoload": {
        "psr-0": {
           "Sirel": "lib/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
